{
  "name": "hackernews",
  "version": "1.0.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/*",
      "!src/**/*-container.{js,jsx,ts,tsx}",
      "!src/**/setup-store.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public/",
      "!<rootDir>/build/"
    ],
    "coverageReporters": ["html"]
  },
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "axios": "^0.19.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.1.3",
    "react-router": "4.4.0-beta.8",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start-client": "react-scripts start",
    "build-client": "workbox generateSW && react-scripts build",
    "build": "npm run build-client && cd host && npm install && npm run build",
    "start": "cd host && npm run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "node-sass": "^7.0.0",
    "workbox-cli": "^5.1.3",
    "redux-logger": "^3.0.6"
  }
}
